{
  "name": "grip-app",
  "version": "0.1.2",
  "description": "GeOM client application for on-site operators.",
  "author": "NEXIO INFORMATICA SRL",
  "contributors": [
    {
      "name": "Luca Parolari",
      "email": "luca.parolari@nexio.cloud",
      "url": "https://github.com/lparolari"
    }
  ],
  "private": true,
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "gen:manifest": "node scripts/generateAppJson.js",
    "gen:dotenv": "source scripts/generateEnv.sh",
    "start": "expo start",
    "start:android": "expo start --android",
    "start:ios": "expo start --ios",
    "start:web": "expo start --web",
    "eject": "expo eject",
    "lint": "tsc --noEmit --skipLibCheck && eslint --ext .ts,.tsx App",
    "test": "echo Skipped",
    "publish:staging": "expo publish --release-channel default",
    "publish:prod": "expo publish --release-channel production",
    "build:staging": "source scripts/didYouCheck.sh && expo:build -t app-bundle --release-channel default",
    "build:prod": "./scripts/didYouCheck.sh && expo:build -t app-bundle --release-channel production"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
